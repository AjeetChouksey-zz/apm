                    },
                    "Virtual Network Address Range": {
                        "value": "[variables('virtualNetowrkAddress')]"
                    },
                    "Subnet Name": {
                        "value": "[variables('subnetName')]"
                    },
                    "Subnet Address Range": {
                        "value": "[variables('subnetRange')]"
                    }
                }
            }
        }
      ],
    "outputs": {  }
